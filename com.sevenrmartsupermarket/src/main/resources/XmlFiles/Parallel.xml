<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MultipleTesting" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="com.sevenrmartsupermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="Regression Testing">
		<classes>
			<class name="com.sevenrmartsupermarket.tests.AdminUsersTest"></class>
			<class name="com.sevenrmartsupermarket.tests.HomeTest"></class>

		</classes>
	</test>
	<test name="Sanity Testing">
		<classes>
			<class name="com.sevenrmartsupermarket.tests.ManageUsersTest"></class>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"></class>

		</classes>
	</test>


</suite>